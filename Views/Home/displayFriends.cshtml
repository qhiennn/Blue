@{
    Layout = null;
}
@model List<Project_Blue.Models.ThongTinCaNhan>
@foreach (var items in Model)
{
    <li class="friends-list">
        <div class="grid__img-friends">
            <img class="items__img-friends"
                 src="@items.AnhDaiDien"
                 alt="images" />
        </div>
        <span class="name-of-Friends">@items.TenKhachHang</span>
    </li>
    
}